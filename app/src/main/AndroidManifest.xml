<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ORKESTRAPayCarsPlatformJualBeliMobilBekas"
        tools:targetApi="31">
        <activity
            android:name=".models.ui.MainContentActivity"
            android:exported="false" />
        <activity
            android:name=".MainWelcomeActivity"
            android:exported="false" />
        <activity
            android:name=".models.MainSignUpActivity"
            android:exported="false" />
        <activity
            android:name=".models.MainSignInActivity"
            android:exported="false" />
        <activity
            android:name=".models.ui.MainHomeActivity"
            android:exported="false" />
        <activity
            android:name=".MainStartActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>