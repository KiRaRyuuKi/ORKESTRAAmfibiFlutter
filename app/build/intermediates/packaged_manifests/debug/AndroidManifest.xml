<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.orkestra.paycars"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="21"
        android:targetSdkVersion="33" />

    <permission
        android:name="com.orkestra.paycars.DYNAMIC_RECEIVER_NOT_EXPORTED_PERMISSION"
        android:protectionLevel="signature" />

    <uses-permission android:name="com.orkestra.paycars.DYNAMIC_RECEIVER_NOT_EXPORTED_PERMISSION" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:debuggable="true"
        android:extractNativeLibs="true"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/Theme.PayCarsPlatformJualBeliMobilBekas" >
        <activity
            android:name="com.orkestra.paycars.MainHome"
            android:exported="false" />
        <activity
            android:name="com.orkestra.paycars.MainGetCode"
            android:exported="false" />
        <activity
            android:name="com.orkestra.paycars.MainRegister"
            android:exported="false"
            android:theme="@style/Theme.PayCarsPlatformJualBeliMobilBekas.NoActionBarAndNoStatusBar" />
        <activity
            android:name="com.orkestra.paycars.MainLogin"
            android:exported="false"
            android:theme="@style/Theme.PayCarsPlatformJualBeliMobilBekas.NoActionBarAndNoStatusBar" />
        <activity
            android:name="com.orkestra.paycars.MainWelcome"
            android:exported="false"
            android:theme="@style/Theme.PayCarsPlatformJualBeliMobilBekas.NoActionBarAndNoStatusBar" />
        <activity
            android:name="com.orkestra.paycars.MainSplashScreen"
            android:exported="true"
            android:theme="@style/Theme.PayCarsPlatformJualBeliMobilBekas.NoActionBarAndNoStatusBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <provider
            android:name="androidx.startup.InitializationProvider"
            android:authorities="com.orkestra.paycars.androidx-startup"
            android:exported="false" >
            <meta-data
                android:name="androidx.emoji2.text.EmojiCompatInitializer"
                android:value="androidx.startup" />
            <meta-data
                android:name="androidx.lifecycle.ProcessLifecycleInitializer"
                android:value="androidx.startup" />
        </provider>
    </application>

</manifest>